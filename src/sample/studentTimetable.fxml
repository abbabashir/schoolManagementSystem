<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #008080;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.studentController">
   <children>
      <TableView fx:id="timetab" layoutX="54.0" layoutY="50.0" prefHeight="336.0" prefWidth="492.0">
        <columns>
          <TableColumn fx:id="mondaycolumn" prefWidth="75.0" text="MONDAY" />
          <TableColumn fx:id="tuesdaycolumn" prefWidth="75.0" text="TUESDAY" />
            <TableColumn fx:id="wednesdaycolumn" prefWidth="90.0" text="WEDNESDAY" />
            <TableColumn fx:id="thursdaycolumn" prefWidth="81.0" text="THURSDAY" />
            <TableColumn fx:id="fridaycolumn" prefWidth="86.0" text="FRIDAY" />
            <TableColumn fx:id="saturdaycolumn" prefWidth="84.0" text="SATURDAY" />
        </columns>
      </TableView>
      <Button layoutX="29.0" layoutY="14.0" mnemonicParsing="false" onAction="#loadTimeTable" text="Fetch" />
      <Button fx:id="backbtn" layoutX="515.0" layoutY="14.0" mnemonicParsing="false" onAction="#buttonback" style="-fx-background-color: #FF6347;" text="Back" />
   </children>
</AnchorPane>
